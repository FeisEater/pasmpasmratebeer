<p id="notice"><%= notice %></p>

<h1>Listing Ratings</h1>

<p> Number of ratings: <%= @ratings.count %> </p>

<ul>
    <% @ratings.each do |rating| %>
      <% byebug if rating.beer.nil? %>
      <li> <%= rating %> <%= link_to 'delete', rating, method: :delete, data: { confirm: 'U w0t m8?' } %> </li>
    <% end %>
</ul>

<br>

<%= link_to 'New Rating', new_rating_path %>

